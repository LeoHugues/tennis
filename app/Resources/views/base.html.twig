<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('component/bootstrap/css/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('component/css/style.css') }}">
         <link rel="stylesheet" type="text/css" href="{{ asset('component/css/flag-icon.css') }}">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    </head>
    <body>
    {% block body %}
        <div class="row">
            {% block navbar %}
                    {% include 'FrontEndBundle:Navbar:navbar.html.twig' %}
            {% endblock %}
            <div class="col-lg-9 content">
                {% block content %}
                {% endblock %}
            </div>
        </div>
    {% endblock %}
        <script src="{{ asset('component/jquery-3.1.1.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('component/bootstrap/js/bootstrap.min.js') }}" type="text/javascript"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
